#include<stdio.h>

int main(){
    
int calificacion[5],promedio,i;

for(i = 0; i<5;i++){
    printf("Dame la calificacion %d: ",i + 1);
    scanf("%d",&calificacion[i]);
}
i=0;
while(i<5){
promedio += calificacion[i];
i++;
}
promedio /= 5;
printf("Tu promedio es: %d",promedio);

if (promedio >= 6){
    printf("\nTu promedio es aprobatorio");
}
    else {
    printf("\nTu promedio es reprobatorio");
}


}
